{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "files": [
        "CD Cache.ts",
        "CentralCode.ts",
        "Code.ts",
        "CoreList.ts",
        "GoodsReceiving.ts",
        "Materials.ts",
        "NonCore.ts",
        "PO 3 and 4.ts",
        "POPR 0.ts",
        "POPR 1.ts",
        "POPR 2.ts",
        "PR 3 and 4.ts",
        "PR email.ts",
        "Statics and Vars.ts",
        "SVS.ts",
        "typings/google-apps-script-tests.ts",
        "typings/google-apps-script.base.d.ts",
        "typings/google-apps-script.cache.d.ts",
        "typings/google-apps-script.calendar.d.ts",
        "typings/google-apps-script.charts.d.ts",
        "typings/google-apps-script.contacts.d.ts",
        "typings/google-apps-script.content.d.ts",
        "typings/google-apps-script.document.d.ts",
        "typings/google-apps-script.drive.d.ts",
        "typings/google-apps-script.forms.d.ts",
        "typings/google-apps-script.gmail.d.ts",
        "typings/google-apps-script.groups.d.ts",
        "typings/google-apps-script.html.d.ts",
        "typings/google-apps-script.jdbc.d.ts",
        "typings/google-apps-script.language.d.ts",
        "typings/google-apps-script.lock.d.ts",
        "typings/google-apps-script.mail.d.ts",
        "typings/google-apps-script.maps.d.ts",
        "typings/google-apps-script.optimization.d.ts",
        "typings/google-apps-script.properties.d.ts",
        "typings/google-apps-script.script.d.ts",
        "typings/google-apps-script.sites.d.ts",
        "typings/google-apps-script.spreadsheet.d.ts",
        "typings/google-apps-script.types.d.ts",
        "typings/google-apps-script.ui.d.ts",
        "typings/google-apps-script.url-fetch.d.ts",
